{
  "dependencies": {
    "gulp": "3.9.1",
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "@evan.network/angular-gulp": "^1.2.2",
    "@evan.network/api-blockchain-core": "^1.6.1",
    "@evan.network/smart-contracts-core": "^1.4.0",
    "@evan.network/ui-angular-core": "^1.6.1",
    "@evan.network/ui-angular-libs": "^1.3.1",
    "@evan.network/ui-angular-sass": "^1.6.0",
    "@evan.network/ui-dapp-browser": "^1.6.3",
    "ipfs-api": "^17.5.0",
    "web3": "1.0.0-beta.37"
  },
  "keywords": [
    "javascript",
    "typescript",
    "API",
    "Angular 5",
    "Ionic",
    "DApp",
    "SASS",
    "tutorial"
  ],
  "license": "Apache-2.0",
  "name": "@evan.network/sample-dapps-angular",
  "scripts": {
    "dapp-build": "gulp --cwd . --gulpfile node_modules/@evan.network/angular-gulp/dapp.js build --dapp",
    "dapp-sass": "gulp --cwd . --gulpfile node_modules/@evan.network/angular-gulp/dapp.js sass --dapp",
    "dapps-build": "gulp --cwd . --gulpfile node_modules/@evan.network/angular-gulp/dapps.js dapps-build",
    "dapps-serve": "gulp --cwd . --gulpfile node_modules/@evan.network/angular-gulp/dapps.js dapps-build --serve",
    "deploy": "gulp --gulpfile node_modules/@evan.network/ui-dapp-browser/gulp/deployment.js deploy --config",
    "deploy-to-contract": "node scripts/create-contract.js --dapp",
    "serve": "gulp --gulpfile node_modules/@evan.network/ui-dapp-browser/gulp/serve.js serve"
  },
  "version": "1.0.0"
}